{
  "collectCoverage": true,
  "collectCoverageFrom": [
      "./**/*.js"
  ],
  "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      "<rootDir>/src/app/main.ts",
      ".mock.ts",
      "jsreport",
      "coverage",
      "multer"
  ],
  "coverageThreshold": {
      "global": {
          "branches": 20,
          "functions": 20,
          "lines": 20,
          "statements": 20
      }
  }
}